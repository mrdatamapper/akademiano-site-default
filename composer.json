{
  "name": "datamapper/site-default",
  "description": "datamapper site default",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Vasiliy Shvakin",
      "email": "mrdatamapper@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sites\\_Default\\": "src"
    }
  },
  "provide": {
        "akademiano/site-default-implementation": "1.0.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:mrdatamapper/datamapper-site-all.git"
    }
  ],
  "require": {
    "akademiano/core": "^1.0.0-beta.6",
    "datamapper/site-all": "dev-master as 1.0.x-dev"
  },
  "require-dev": {
    "codeception/codeception": "^2.2.10",
    "codeception/c3": "^2.0.10",
    "satooshi/php-coveralls": "^1.0",
    "mockery/mockery": "^0.9.9"
  },
 "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "public/assets/vendor",
        "bower-asset-library": "public/assets/vendor"
      },
      "ignore-files": {
        "bower-asset/bootstrap": [
          "/.*",
          "_config.yml",
          "CNAME",
          "composer.json",
          "CONTRIBUTING.md",
          "docs",
          "js/tests",
          "test-infra",
          "fonts",
          "grunt",
          "js",
          "less",
          "Gruntfile.js",
          "package.json",
          "package.js"
        ],
        "bower-asset/jquery": [
          "**/.*",
          "build",
          "dist/cdn",
          "speed",
          "test",
          "*.md",
          "AUTHORS.txt",
          "Gruntfile.js",
          "package.json",
          "src"
        ]
      }
    }
  }
}
